<Window x:Class="Online_hardware_store.Windows.SubcategoriesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Online_hardware_store.Windows"
        mc:Ignorable="d"
        Title="SubcategoriesWindow" Height="450" Width="800"
        ResizeMode="NoResize">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="20*"/>
            <RowDefinition Height="70*"/>
        </Grid.RowDefinitions>
        <Frame Grid.Row="0"
               Name="frameTopPanel"/>
        <Frame Grid.Row="1"
               Name="frameSubcategories"/>
    </Grid>
</Window>
